{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "yess/restaurants-ai/(.*)": "<rootDir>/../libs/restaurants-ai/src/$1",
    "yess/restaurants-ai": "<rootDir>/../libs/restaurants-ai/src"
  }
}